# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

GET         /                       controllers.HomeController.index
GET         /exercise/:id/edit      controllers.ExerciseController.edit(id: Long)
POST        /exercise/:id/update    controllers.ExerciseController.update(id: Long)
POST        /addexercise            controllers.HomeController.addExercise
POST        /login                  controllers.HomeController.login
POST        /logout                 controllers.HomeController.logout

# Map static resources from the /public folder to the /assets URL path
GET         /assets/*file        controllers.Assets.versioned(path="/public", file: Asset)
